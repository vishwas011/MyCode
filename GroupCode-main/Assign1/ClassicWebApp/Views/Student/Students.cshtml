<html>

<head>
    <title>Student Details</title>
</head>

<body>
    <h1>Student Details</h1>
    <table border="1">
        <tr>
            <th>Rollno</th>
            <th>Name</th>
            <th>Age</th>
            <th>Grade</th>
        </tr>
 @foreach(var item in ViewBag.Student)
 {
    <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Age</td>
        <td>@item.Grade</td>
    </tr>
 }   
        

</body>

</html>